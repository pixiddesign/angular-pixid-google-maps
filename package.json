{
  "name": "angular-pixid-google-maps",
  "version": "1.0.2",
  "description": "Angular 1.x Google Maps library by Pixid Design",
  "main": "./lib/main.js",
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "webpack --config config/webpack.prod.js  --progress --profile --colors --display-error-details --display-cached --bail",
    "clean": "npm cache clean && rimraf -- node_modules dist",
    "clean:dist": "rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean:lib": "rimraf -- lib",
    "clean:start": "npm start",
    "compile": "webpack --config config/webpack.lib.js  --progress --profile --colors --display-error-details --display-cached --bail",
    "prebuild:dev": "npm run clean:dist",
    "prebuild:prod": "npm run clean:dist",
    "preclean:install": "npm run clean",
    "preclean:start": "npm run clean",
    "precompile": "npm run clean:lib",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
    "server:dev:hmr": "npm run server:dev -- --hot",
    "server:prod": "http-server dist --cors",
    "start": "npm run server:dev"
  },
  "repository": {
    "type": "git",
    "url": "pixiddesign/angular-pixid-google-maps"
  },
  "author": "Andrew Bennett <andrew@pixid.com>",
  "license": "MPL2",
  "dependencies": {
    "angular": "*",
    "jquery": "*"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^2.1.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.6.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-angular": "^1.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.15.0",
    "http-server": "^0.9.0",
    "image-webpack-loader": "^1.7.0",
    "ng-annotate-loader": "^0.1.0",
    "ngtemplate-loader": "^1.3.1",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.2",
    "pug": "^2.0.0-alpha6",
    "pug-html-loader": "^1.0.5",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^0.12.0"
  }
}
